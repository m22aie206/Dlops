      "cell_type": "code",
      "source": [
        "def sigmoid(x):\n",
        "    return 1 / (1 + np.exp(-x))\n",
        "\n",
        "r_values = [-3.5, -1.2, 0, 2.8, -4.1, 1.5, -0.7, 3.2, -2.4, 4.6]\n",
        "\n",
        "for val in r_values:\n",
        "    print(f\"Sigmoid({val}) = {sigmoid(val)}\")"
